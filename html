			<script>
			function loadPosts () {
			    var key = "api_key=xI4eTiMLLlJN1SvPEGCw4aweqkraMLECHRcFaM2lrUFeuyBGyZ&tag=moodboard-text";
			    var api = "https://api.tumblr.com/v2/blog/codingwitherica.tumblr.com/";
			    var retrieve_more = function (offset) {
			        $.getJSON(api + "posts/text?callback=?&limit=20&offset=" + offset + "&" + key,function(data) {
			            $.each(data.response.posts, function(i, item) {
			                var content = item.body;
			                $(".moodboard_caption").append('<p>' + content + '</p>')
			            });

			            if (data.response.posts.length == 20) {
			                retrieve_more(offset + 20);
			            }
			        });
			    };
			    
			    retrieve_more(0);
			}

			loadPosts();
			</script>
	         
